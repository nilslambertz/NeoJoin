package tools.vitruv.neojoin.expression_parser

import "http://www.eclipse.org/xtext/xbase/Xbase"

pattern skipOneReferencePatternTest(
	base: XExpression,
    midCall: XMemberFeatureCall,
    root: XMemberFeatureCall
) {
	XMemberFeatureCall(expr);
	XMemberFeatureCall.memberCallTarget(root, midCall);
	
	XMemberFeatureCall(midCall);
	XMemberFeatureCall.memberCallTarget(midCall, base);
}
