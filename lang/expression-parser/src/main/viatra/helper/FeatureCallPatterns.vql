package viatra.helper

import "http://www.eclipse.org/xtext/xbase/Xbase" as xbase

pattern MemberFeatureCall(
	source: XAbstractFeatureCall,
	featureCall: xbase::XMemberFeatureCall) {
	xbase::XMemberFeatureCall(featureCall);
	xbase::XMemberFeatureCall.memberCallTarget(featureCall, source);
	xbase::XAbstractFeatureCall(source);
}